<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybooklibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bookicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bookicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyBookLibrary">
        <activity android:name=".WebsiteActivity"></activity>
        <activity android:name=".CurrentlyReadingActivity"
            android:label="@string/activity_currently_reading_books"/>
        <activity android:name=".FavoriteActivity"
            android:label="@string/activity_favorite_books"/>
        <activity android:name=".WishlistActivity"
            android:label="@string/activity_want_to_read_books"/>
        <activity android:name=".AlreadyReadBookActivity"
            android:label="@string/activity_already_read"/>
        <activity android:name=".BookActivity"
            android:label="@string/activity_book_details"/>
        <activity android:name=".AllBooksActivity"
            android:label="@string/activity_all_books"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>